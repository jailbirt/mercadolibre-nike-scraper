# Configuraciones del script
# País de MercadoLibre (mla = Argentina, mlm = México, mlb = Brasil, etc.)
COUNTRY_CODE=mla

# Configuración de la búsqueda
SEARCH_TERM=zapatillas nike
MAX_RESULTS=50
PRICE_MIN=0
PRICE_MAX=50000

# Opciones del navegador
HEADLESS=true
SAVE_SCREENSHOTS=true
SCREENSHOTS_DIR=./screenshots

# Opciones de salida
OUTPUT_DIR=./output
OUTPUT_FILENAME=nike_zapatillas